   "source": [
    "from math import factorial\n",
    "\n",
    "def lattice_paths_factorial(n: int) -> int:\n",
    "    return factorial(2 * n) // (factorial(n) * factorial(n))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    n = 20\n",
    "    print(lattice_paths_factorial(n))"
   ]
