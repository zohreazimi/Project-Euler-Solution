   "source": [
    "import math\n",
    "\n",
    "factorial_100 = math.factorial(100)\n",
    "\n",
    "factorial_str = str(factorial_100)\n",
    "digit_sum = 0\n",
    "for digit_char in factorial_str:\n",
    "    digit_sum += int(digit_char)\n",
    "\n",
    "print(\"result =\", digit_sum)\n"
   ]
  }
