   "source": [
    "def diagonal_sum(n):\n",
    "    total = 1\n",
    "    for i in range(3, n + 1, 2):\n",
    "        total += 4 * i**2 - 6 * i + 6\n",
    "    return total\n",
    "\n",
    "print(diagonal_sum(1001))\n"
   ]
